  </urls>
  <license>AGPL</license>
  <maintainer>
    <author>Andrew Wasson | Luna Design</author>
    <email>awasson@lunadesign.org</email>
  </maintainer>
  <releaseDate>2013-01-06</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
